<?xml version='1.0' encoding='UTF-8'?>
<Content xmlns="http://www.hp.com/PC/REST/API">
  <WorkloadType>
    <Type>real-world</Type>
    <SubType>by group</SubType>
  </WorkloadType>
  <LGDistribution>
    <Type>all to each group</Type>
    <Amount>12</Amount>
  </LGDistribution>
  <Groups>
    <Group>
      <Name>SampleGroup</Name>
      <Vusers>50</Vusers>
      <Script>
        <ID>200</ID>
        <ProtocolType>Web - HTTP/HTML</ProtocolType>
      </Script>
      <RTS>
        <Pacing>
          <NumberOfIterations>3</NumberOfIterations>
          <StartNewIteration Type="fixed delay">
            <DelayOfSeconds>5</DelayOfSeconds>
          </StartNewIteration>
        </Pacing>
        <ThinkTime Type="ignore"/>
        <Log Type="standard">
          <LogOptions Type="on error">
            <CacheSize>1</CacheSize>
          </LogOptions>
        </Log>
      </RTS>
      <Scheduler>
        <Actions>
          <Action>
            <StartGroup Type="immediately"/>
          </Action>
          <Action>
            <Initialize Type="gradually">
              <TimeInterval>
                <Seconds>30</Seconds>
              </TimeInterval>
              <Vusers>10</Vusers>
              <WaitAfterInit/>
            </Initialize>
          </Action>
          <Action>
            <StartVusers Type="simultaneously">
              <Vusers>50</Vusers>
            </StartVusers>
          </Action>
          <Action>
            <Duration Type="run for">
              <TimeInterval>
                <Seconds>30</Seconds>
              </TimeInterval>
            </Duration>
          </Action>
          <Action>
            <StopVusers Type="simultaneously">
              <Vusers>50</Vusers>
            </StopVusers>
          </Action>
        </Actions>
      </Scheduler>
    </Group>
  </Groups>
  <SLA>
    <TransactionResponseTimePercentile>
      <Percentile>95</Percentile>
      <Transactions>
        <Transaction>
          <Name>landing_page</Name>
          <Threshold>5.0</Threshold>
        </Transaction>
        <Transaction>
          <Name>login</Name>
          <Threshold>8.0</Threshold>
        </Transaction>
      </Transactions>
    </TransactionResponseTimePercentile>
    <ErrorsPerSecond>
      <LoadCriterion>hits_per_second</LoadCriterion>
      <LoadValues>
        <Betweens>
          <Between>
            <From>5.0</From>
            <To>10.0</To>
          </Between>
          <Between>
            <From>10.0</From>
            <To>15.0</To>
          </Between>
          <Between>
            <From>15.0</From>
            <To>20.0</To>
          </Between>
        </Betweens>
        <LessThan>5.0</LessThan>
        <GreaterThanOrEqual>20.0</GreaterThanOrEqual>
      </LoadValues>
      <Thresholds>
        <LessThanThreshold>2.0</LessThanThreshold>
        <BetweenThreshold>
          <Threshold>
            <Threshold>3.0</Threshold>
            <Threshold>4.0</Threshold>
            <Threshold>5.0</Threshold>
          </Threshold>
        </BetweenThreshold>
        <GreaterThanOrEqualThreshold>6.0</GreaterThanOrEqualThreshold>
      </Thresholds>
    </ErrorsPerSecond>
    <TotalHits>
      <Threshold>20.0</Threshold>
    </TotalHits>
    <AverageHitsPerSecond>
      <Threshold>50.0</Threshold>
    </AverageHitsPerSecond>
    <TotalThroughput>
      <Threshold>80000.0</Threshold>
    </TotalThroughput>
    <AverageThroughput>
      <Threshold>5000.0</Threshold>
    </AverageThroughput>
  </SLA>
  <AnalysisTemplate>
    <ID>10</ID>
  </AnalysisTemplate>
  <AutomaticTrending>
    <ReportId>5</ReportId>
    <MaxRunsInReport>20</MaxRunsInReport>
    <TrendRangeType>CompleteRun</TrendRangeType>
    <MaxRunsReachedOption>DeleteFirstSetNewBaseline</MaxRunsReachedOption>
  </AutomaticTrending>
</Content>
